<UserControl
    x:Class="XWingSquadronBuilder_v4.Presentation.UserControls.SquadronPrintControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:XWingSquadronBuilder_v4.Presentation.UserControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:XWingModels="using:XWingSquadronBuilder_v4.Presentation.ViewModels.XWingModels.Interfaces"
    xmlns:interfaces="using:XWingSquadronBuilder_v4.Interfaces"
    xmlns:userControls="using:XWingSquadronBuilder_v4.Presentation.UserControls"
    xmlns:converters="using:XWingSquadronBuilder_v4.Presentation.Converters"
    xmlns:Interactivity="using:Microsoft.Xaml.Interactivity"
    xmlns:Interactions="using:Microsoft.Xaml.Interactions.Core"
    mc:Ignorable="d" FontFamily="Segoe UI" >

    
        <RichTextBlock x:Name="TitleTextBlock"
                       FontFamily="Segoe UI">
            <Paragraph FontFamily="Segoe UI">
                <InlineUIContainer FontFamily="Segoe UI">
                    <TextBlock Text="{x:Bind ViewModel.Squadron.Name, FallbackValue=SquadronName}" FontFamily="Segoe UI"/>
                </InlineUIContainer>
            </Paragraph>
        <Paragraph FontFamily="Segoe UI">
            
        </Paragraph>
        </RichTextBlock>
    
</UserControl>
